<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog confirmButtonText="Ajuda" id="_IDGenSelectDialog1"
		title="{i18n>titleSHCategoria}"
		contentHeight= "50%"
		search="onValueHelpSearch"
		confirm="onValueHelpClose"
		cancel="onValueHelpClose"
		showClearButton= "false"
		items="{/Categorias}">

		<StandardListItem id="_IDGenStandardListItem1"
			title="{Category}"
			description="{Description}"
			icon="sap-icon://product"
			iconDensityAware="false" 
			iconInset="false" />
			
	</SelectDialog>
</core:FragmentDefinition>